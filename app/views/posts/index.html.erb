<% if user_signed_in?%>
  <h1>My Feed</h1>
  
  <div id="posts">
    <% @posts.each do |post| %>
      
      <p>
      <div class="card">
  <div class="card-header">
    <%= post.user.first_name%> <%=post.user.last_name %>
    <br>
    <%= post.created_at.strftime("%m/%d/%Y") %>
  </div>
  <div class="card-body">
    <p class="card-text"><%= post.body %></p>
    <%= link_to "Show this post", post %>
  </div>
</div>
        
      </p>
    <% end %>
  </div>
  
  <%= link_to "New post", new_post_path %>

<% else %>

  <h1>FakeBook</h1>
  <p> Sign up or log in by clicking the links above</p>
  
<% end %>